<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="boardMapper">

	<!-- 관리자 신고 승인 -->

	<update id="mDeBApply" parameterType="_int">
		update board
		set status = 'N'
		where bNo = #{ bNo }
	</update>

	
	
</mapper>
